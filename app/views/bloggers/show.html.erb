<h1><%= @blogger.name %></h1>
<div><%= @blogger.age %></div>
<div><%= @blogger.bio %></div>
<h4>Featured Post:</h4>
<%= @post.featured_post %>
Likes: <%= @post.featured_post.likes %><br>
<br>
<h4>All Posts:</h4>
<ul>
<% @blogger.posts.each do |post| %>
    <li><%= post.title %><br>
    Likes: <%= post.likes %><br>
    <br></li>
<% end %>
</ul>